{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "functions": {
    "api/ssr.mjs": {
      "includeFiles": [
        "public/assets/**/*"
      ]
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/api/ssr" }
  ]
}
