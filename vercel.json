{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "cache-control", "value": "public,max-age=31536000,immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/ssr" }
  ]
}
