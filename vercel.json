{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/api/ssr" }
  ]
}
