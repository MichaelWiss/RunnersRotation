{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "functions": {
    "api/ssr.mjs": {
      "includeFiles": [
        "build/client/**",
        "build/server/**",
        "public/assets/**"
      ]
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/api/ssr" }
  ]
}
